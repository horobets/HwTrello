<suite name="Trello Suite">
    <test name="Login">
        <classes>
            <class name="regression.legacy.LoginTests">
                <methods>
                    <exclude name="logout"></exclude>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Create And Delete Board">
        <classes>
            <class name="regression.legacy.CreateDeleteBoardTest">
                <methods>
                    <exclude name="createBoard"></exclude>
                </methods>
            </class>
        </classes>
    </test>
